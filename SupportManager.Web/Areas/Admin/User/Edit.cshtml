@model SupportManager.Web.Areas.Admin.User.Edit.Command
@using SupportManager.Web.Infrastructure

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>User</h4>
        <hr/>
        @Html.HiddenFor(m => m.Id)
        @Html.FormBlock(m => m.DisplayName)
        @Html.FormBlock(m => m.Login)
        @Html.FormBlock(m => m.PrimaryEmailAddress)
        @Html.FormBlock(m => m.PrimaryPhoneNumber)
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Edit" class="btn btn-default"/>
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to list", "Index", "User")
</div>